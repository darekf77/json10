{
  "tnp": {
    "resources": [],
    "type": "isomorphic-lib",
    "isCommandLineToolOnly": true,
    "overrided": {
      "includeAsDev": [
        "morphi",
        "@angular.*",
        "webpack",
        "webpack-dev-server",
        "html-webpack-plugin",
        "ts-loader",
        "babel.*",
        "mocha",
        "@types/mocha",
        "chai",
        "ts-sinon"
      ],
      "includeOnly": [
        "lodash",
        "lodash-walk-object",
        "typescript-class-helpers",
        "@types/lodash"
      ]
    }
  },
  "name": "json10",
  "description": "Handle circural references, comments and many more inside JSON",
  "version": "0.0.6",
  "scripts": {
    "test": "mocha --require ts-node/register src/**/*.spec.ts",
    "test:watch": "mocha --compilers ts-node/register src/**/*.spec.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/json10.git"
  },
  "keywords": [
    "ng2-rest",
    "docs",
    "documentation"
  ],
  "author": "Dariusz Filipiak",
  "bugs": {
    "url": "https://github.com/darekf77/json10/issues"
  },
  "homepage": "https://github.com/darekf77/json10#readme",
  "husky": {
    "hooks": {
      "pre-push": "tnp deps:show:if:standalone"
    }
  }
}
